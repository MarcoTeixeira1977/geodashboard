{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": [
      "src/scripts",
      "package.json",
      "README.md"
    ],
    "includePattern": ".js$"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "destination": "docs/",
    "encoding": "utf8",
    "private": false,
    "recurse": true,
    "template": "./node_modules/docdash"
  }
}
