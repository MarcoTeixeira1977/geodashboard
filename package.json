  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^4.0.0",
    "css-loader": "^0.25.0",
    "docdash": "^0.4.0",
    "eslint": "^3.8.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-loader": "^1.6.0",
    "eslint-plugin-import": "^1.16.0",
    "extract-text-webpack-plugin": "^1.0.1",
